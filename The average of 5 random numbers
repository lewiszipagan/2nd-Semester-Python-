from random import  uniform

num1=uniform(75,100)
num2=uniform(75,100)
num3=uniform(75,100)
num4=uniform(75,100)
num5=uniform(75,100)

print("num1",round(num1,2))
print("num2",round(num2,2))
print("num3",round(num3,2))
print("num4",round(num4,2))
print("num5",round(num5,2))
average=(num1+num2+num3+num4+num5)/5

print("The average of 5 random numbers are",round(average,2))
